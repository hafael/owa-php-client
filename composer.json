{
    "name": "hafael/owa-php-client",
    "type": "library",
    "description": "PHP Client for Open Web Analytics REST Api",
    "keywords": [ "open web analytics", "php", "client", "open", "web", "analytics", "rest", "api", "open source" ],
    "homepage": "https://github.com/hafael/owa-php-client",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "VerdeIT",
            "email": "villa655321verde@gmail.com",
            "homepage": "https://github.com/hafael"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": ">=6.0",
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0.0",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Hafael\\OpenWebAnalytics\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/OpenWebAnalyticsTest.php"
        ],
        "psr-4": {
            "Hafael\\OpenWebAnalytics\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
